    "arweave": "^1.11.8",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "gray-matter": "^4.0.3",
    "iframe-resizer-react": "^1.1.0",
    "image-size": "^1.0.2",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-image-size": "^2.0.0",
    "react-markdown": "^8.0.3",
    "rehype-raw": "^6.1.1",
    "remark": "^14.0.2",
    "slug": "^8.2.2",
    "unified": "^10.1.2",
    "wagmi": "^0.9.0"
