  "version": "1.0.0",
  "private": true,
  "author": "Matthias Jordan <hey@iammatthias.com> (@iammatthias)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "turbo-dev": "turbo dev",
    "tubo-build": "turbo build",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"./**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"./**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
